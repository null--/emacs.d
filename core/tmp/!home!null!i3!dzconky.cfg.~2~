background yes
out_to_console yes
out_to_x no
update_interval 1
total_run_times 0
use_spacer none
double_buffer yes
TEXT
^fg(\#0099e6)[${time %m-%d-%y %H:%M} ${time %S}][b: ${exec acpi -a | awk -F" " '{print $3}' | sed s/on-line/+/g | sed s/off-line/x/g} ${battery_percent BAT1}%][c: ${cpu cpu1}%][m: ${memperc}%][d: ${fs_used_perc /}%][${exec ls /tmp/ | grep emacs}][e: ${if_existing /proc/net/route eth0}${upspeed eth0} ${downspeed eth0}${else} N/A ${endif}][w: ${if_existing /proc/net/route wlan0}${upspeed wlan0} ${downspeed wlan0}${else} N/A ${endif}][p: ${if_existing /proc/net/route ppp0}${upspeed ppp0} ${downspeed ppp0}${else} N/A ${endif}]
